{
  "name": "list_entry_children",
  "description": "列出课程内容树中某个具体条目的所有子节点。",
  "parameters": {
    "type": "object",
    "properties": {
      "course": {
        "type": "string",
        "description": "课程名称，可模糊匹配"
      },
      "entry": {
        "type": "string",
        "description": "要展开的节点标题（Entry 名称，可模糊匹配）"
      }
    },
    "required": ["course", "entry"]
  }
}